{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["*"]
    }
  }
}
